    RewriteRule ^(.*)/(.*)/$ /pioneer_dev/index.php?pageType=$1&dtaction=$2 [L,QSA]
    RewriteRule ^(.*)/$ /pioneer_dev/index.php?pageType=$1 [L,QSA]
    #RewriteRule ^(.*)\.html$ /pioneer_dev/index.php?pageType=$1 [L,QSA]
    
